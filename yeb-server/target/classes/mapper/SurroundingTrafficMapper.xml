<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.server.mapper.SurroundingTrafficMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.server.pojo.SurroundingTraffic">
        <result column="Community" property="Community" />
        <result column="district" property="district" />
        <result column="name" property="name" />
        <result column="class1" property="class1" />
        <result column="class2" property="class2" />
        <result column="class3" property="class3" />
        <result column="latitude" property="latitude" />
        <result column="longitude" property="longitude" />
        <result column="distance" property="distance" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        Community, district, name, class1, class2, class3, latitude, longitude, distance
    </sql>

</mapper>
